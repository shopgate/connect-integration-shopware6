<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="sg_connect_config_css" type="constant">Shopgate\ConnectSW6\Config::KEY_CSS</parameter>
    </parameters>
    <services>
        <defaults autowire="true" autoconfigure="true"/>
        <service id="Shopgate\ConnectSW6\Subscribers\SystemConfigSubscriber">
            <argument key="$cssConfigKey" type="string">%sg_connect_config_css%</argument>
            <tag name="kernel.event_subscriber"/>
        </service>
    </services>
</container>
